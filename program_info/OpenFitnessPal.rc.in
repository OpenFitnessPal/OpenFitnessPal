#ifndef WIN32_LEAN_AND_MEAN
#define WIN32_LEAN_AND_MEAN
#endif
#include <windows.h>

IDI_ICON1               ICON    DISCARDABLE     "@OFP_APP_NAME@.ico"
10255 RT_MANIFEST "@OFP_APP_NAME@.manifest"

VS_VERSION_INFO VERSIONINFO
FILEVERSION     @OFP_GIT_TAG_NUMERO@
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_APP
BEGIN
        BLOCK "StringFileInfo"
        BEGIN
                BLOCK "000004b0"
                BEGIN
                        VALUE "CompanyName", "@OFP_ORG_NAME@"
                        VALUE "FileDescription", "@OFP_APP_NAME@"
                        VALUE "FileVersion", "@OFP_GIT_TAG@"
                        VALUE "ProductName", "@OFP_APP_NAME@"
                        VALUE "ProductVersion", "@OFP_GIT_TAG@"
                END
        END
        BLOCK "VarFileInfo"
        BEGIN
                VALUE "Translation", 0x0000, 0x04b0 // Unicode
        END
END
